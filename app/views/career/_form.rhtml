<%= error_messages_for 'career' %>

<!--[form:career]-->
<div class="row">
<label for="name" class="label">Nombre de la carrera <span class="required">*</span></label>
<%= text_area 'career', 'name', 'size' => '40x2', 'tabindex'=> 1, 'id' => 'name' %>
</div>

<div class="row">
<label for="degree_id" class="label"><%= get_label('degree_id') %> <span class="required">*</span></label>
<div id="degree_id">
<%= simple_select('career', Degree, 3) %>
</div>
</div>


